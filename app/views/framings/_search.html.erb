<form>
  <div class="input-group">
    <span class="input-group-addon"><i class="glyphicon glyphicon-search"></i></span>
    <input id="myInput" type="text" class="form-control" name="search" placeholder="Search..">
  </div>
</form>
<br>
<div id="table-wrapper">
<table class="table table-bordered table-striped table-hover" id="table">
<thead>
  <tr>
    <% Framing.column_names.each do |column_name| %>
      <% if is_useful_column_name column_name %>
        <th><%= beautify_name column_name %></th>
      <% end %>
    <% end %>
  </tr>
</thead>
    
<tbody id="myTable">
  <% @framings.each do |framing| %>
      <tr data-href="<%= framing_path(framing) %>">
        <% framing.attributes.each do |column_name, value| %>
          <% if is_useful_column_name column_name %>
            <td class="col-md-2" id=<%= "value-#{column_name}" %>>
              <%= value %>
            </td>
          <% end %>
        <% end %>
      </tr>
  <% end %>
</tbody>
</table>
</div>
